  },
  "homepage": "https://github.com/pallemach2/WebConnect#readme",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@swc/core": "^1.7.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
